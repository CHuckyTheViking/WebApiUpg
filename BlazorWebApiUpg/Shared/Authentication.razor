@code {
    private string accessToken { get; set; }


    protected override async Task OnInitializedAsync()
    {
        try
        {
            accessToken = await localStorage.GetItemAsync<string>("authenticated");

            if (accessToken != null)
            {
                Http.DefaultRequestHeaders.Add("Authorization", String.Format($"Bearer " + accessToken));
            }



            if (accessToken == null)
            {
                NavManager.NavigateTo("/login");
            }

        }
        catch
        {

        }



    }

}
